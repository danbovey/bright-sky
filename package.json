{
  "name": "my-razzle-app",
  "version": "4.2.15",
  "license": "MIT",
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider razzle start",
    "start": "NODE_ENV=production node build/server.js",
    "build": "NODE_ENV=production NODE_OPTIONS=--openssl-legacy-provider PORT=8080 razzle build --noninteractive",
    "test": "razzle test --env=jsdom",
    "deploy": "yarn build && flyctl deploy"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@svgr/webpack": "5",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.3.6",
    "express": "^4.17.1",
    "offline-plugin": "^5.0.7",
    "razzle-plugin-offline": "^0.0.1-alpha.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.39.1",
    "react-router-dom": "^5.2.0",
    "recharts": "^2.1.12",
    "styled-components": "^5.3.5",
    "use-local-storage-state": "17"
  },
  "devDependencies": {
    "@types/express": "^4.17.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.25",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "babel-plugin-styled-components": "^2.0.7",
    "babel-preset-razzle": "4.2.15",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "html-webpack-plugin": "^4.5.2",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.7.1",
    "razzle": "^4.2.17",
    "razzle-dev-utils": "^4.2.17",
    "razzle-plugin-typescript": "^4.2.17",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "transform": {
      "\\.(ts|tsx)$": "ts-jest",
      "\\.css$": "<rootDir>/node_modules/razzle/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/node_modules/razzle/config/jest/fileTransform.js"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.(ts|js)?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  }
}
